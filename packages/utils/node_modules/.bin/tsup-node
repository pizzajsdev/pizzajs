#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/pizzajs/pizzajs/node_modules/.pnpm/tsup@8.4.0_jiti@2.4.2_postcss@8.5.3_typescript@5.8.2/node_modules/tsup/dist/node_modules:/home/runner/work/pizzajs/pizzajs/node_modules/.pnpm/tsup@8.4.0_jiti@2.4.2_postcss@8.5.3_typescript@5.8.2/node_modules/tsup/node_modules:/home/runner/work/pizzajs/pizzajs/node_modules/.pnpm/tsup@8.4.0_jiti@2.4.2_postcss@8.5.3_typescript@5.8.2/node_modules:/home/runner/work/pizzajs/pizzajs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/pizzajs/pizzajs/node_modules/.pnpm/tsup@8.4.0_jiti@2.4.2_postcss@8.5.3_typescript@5.8.2/node_modules/tsup/dist/node_modules:/home/runner/work/pizzajs/pizzajs/node_modules/.pnpm/tsup@8.4.0_jiti@2.4.2_postcss@8.5.3_typescript@5.8.2/node_modules/tsup/node_modules:/home/runner/work/pizzajs/pizzajs/node_modules/.pnpm/tsup@8.4.0_jiti@2.4.2_postcss@8.5.3_typescript@5.8.2/node_modules:/home/runner/work/pizzajs/pizzajs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
