{
  "name": "@pizzajsdev/react-router-hono",
  "version": "0.3.2",
  "description": "React Router v7+ adapter for Hono, compatible with Node and Vercel servers.",
  "homepage": "https://github.com/pizzajsdev/pizzajs#readme",
  "bugs": {
    "url": "https://github.com//pizzajsdev/pizzajs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pizzajsdev/pizzajs.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup --clean",
    "postbuild": "publint",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.1",
    "@vercel/nft": "^0.29.2",
    "esbuild": "^0.25.4",
    "hono": "^4.7.9",
    "minimatch": "^10.0.1",
    "vite": "^6.3.5"
  },
  "devDependencies": {
    "@react-router/dev": "^7.6.0",
    "@react-router/node": "^7.6.0",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "isbot": "^5.1.27",
    "publint": "^0.3.12",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@react-router/node": "^7.6.0",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.0"
  }
}
